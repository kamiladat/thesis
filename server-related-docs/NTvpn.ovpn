#################################################
# OpenVPN config file for NTUU (Client)         #
#                                               #
# On Windows, you might want to rename this     #
# file so it has a .ovpn extension              #
#                                               #
# Comments are preceded with '#' or ';'         #
#################################################


# !!!!!!!!!!!!!!!!!!!!!!!! 4 Windows
route-method exe
route-delay 2

# 2023-07-28 wh - Bug: WINS gets not pushed to client
disable-dco 
# !!!!!!!!!!!!!!!!!!!!!!!! 4 Windows


client

dev tun
proto udp
sndbuf 0
rcvbuf 0

remote 134.60.124.87 1234
remote 134.60.124.86 1234
remote-random

resolv-retry infinite
nobind

remote-cert-tls server 

tls-client
tls-version-min 1.2
;tls-auth ta.key 1

auth SHA512
;cipher AES-256-CBC
cipher AES-256-GCM
auth-user-pass
key-direction 1

;compress lz4
;allow-compression asym

persist-key
persist-tun
mute-replay-warnings
setenv opt block-outside-dns
mute 20
verb 3
